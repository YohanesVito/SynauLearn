# Supabase Configuration
# For browser/Next.js (NEXT_PUBLIC_ prefix)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# For Node.js scripts (duplicate without prefix)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OnchainKit Configuration
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# WalletConnect Configuration
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Pinata IPFS Configuration
# Get your API keys from: https://pinata.cloud
PINATA_JWT=your_pinata_jwt_token_here

# Pinata Gateway (optional - defaults to gateway.pinata.cloud)
# For browser/Next.js use NEXT_PUBLIC_ prefix
NEXT_PUBLIC_PINATA_GATEWAY=gateway.pinata.cloud
# For Node.js scripts (setupIPFS.ts) you can use either:
PINATA_GATEWAY=gateway.pinata.cloud

# Optional: Custom Pinata Gateway
# If you have a dedicated gateway, replace gateway.pinata.cloud with your custom domain
# NEXT_PUBLIC_PINATA_GATEWAY=your-custom-gateway.mypinata.cloud
# PINATA_GATEWAY=your-custom-gateway.mypinata.cloud

# Smart Contract Owner Private Key (for setBaseURI script)
# ⚠️  KEEP THIS SECRET! Never commit this to git!
# This is only needed to run the setBaseURI.ts script
# Must be the private key of the contract owner address
OWNER_PRIVATE_KEY=0xYourPrivateKeyHere
